;
; BIND reverse data file for broadcast zone
;
$TTL	604800
@	IN	SOA	ns1.nurl.local. root.nurl.local. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	localhost.
15      IN      PTR     ns1.nurl.local
15      IN      PTR     ns1.nurl.local
15      IN      PTR     www.nurl.local
15      IN      PTR     mail.nurl.local
